;Game over screen definition
[Game Over Screen]
enabled = 1              
storyboard =              ;Game over storyboard filename

;-------------------------------------------------------------------
;Victory screen definition
;This is the screen that is shown when you win a match in arcade mode.
;It will not be displayed on the last match of arcade mode.
[Victory Screen]
enabled = 1
time = 300000                
fadein.time = 0
fadeout.time = 15
;Winner's portrait and name
p1.offset = 330,50
p1.spr = 9000,1
p1.layerno=0
p1.facing = 1
p1.scale = .22,.22
p1.window = 0,0, 1280,720
p1.name.offset = 640,99999;100
p1.name.font = 8,6,0
;Win quote text
winquote.text = "WINZ"
winquote.offset = 640,640
winquote.font = 6,0,0
winquote.window = 10,521, 1241,1014
winquote.scale = 0.7,0.7
;winquote.textwrap = w     
;winquote.layerno=1

;-----------------------
;Victory screen background
[VictoryBGdef] 

[VictoryBG 0]
type  = parallax
spriteno = 2,1
start = 0, 0
width = 400, 1200
yscalestart = 100
yscaledelta = 1
tile  = 1,1
velocity = -1

;-------------------------------------------------------------------
;Win screen definition
;This is the screen that is shown when you win in arcade mode.
;It will not be displayed if the winning character has an ending.
[Win Screen]
enabled = 1               
wintext.text = "MEGA SWAG!"
wintext.font = 8,0,0
wintext.offset = 640,240
wintext.displaytime = -1
wintext.layerno = 2
fadein.time = 32
pose.time = 300
fadeout.time = 64

;-------------------------------------------------------------------
;Game ending screen definition
;This screen shows up after the Win Screen.
;It will not be displayed if the winning character has an ending.
[Default Ending]
enabled = 0              
storyboard =              ;Default ending storyboard filename

;-------------------------------------------------------------------
;Ending credits screen definition
;This screen shows up after the Win Screen and Ending
[End Credits]
enabled = 0              
storyboard =              ;Credits storyboard filename

;-------------------------------------------------------------------
;Survival mode results screen definition
;This is the screen that is shown when you win in survival mode.
;It will not be displayed if the winning character has an ending.
[Survival Results Screen]
enabled = 1
winstext.text = "ROUNDS SURVIVED %i"  
winstext.font = 8,0,0
winstext.offset = 640,240
winstext.displaytime = -1
winstext.layerno = 2
fadein.time = 32
show.time = 300
fadeout.time = 64
roundstowin = 10           ;Number of rounds to get win pose (lose pose otherwise)

